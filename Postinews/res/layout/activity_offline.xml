<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/FrameLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <GridView
        android:id="@+id/gv_storedNews"
        android:layout_width="match_parent"
        android:layout_height="398dp"
        android:layout_gravity="bottom|fill_horizontal" >

    </GridView>

    <ImageButton
        android:id="@+id/ib_system_menue"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:src="@android:drawable/arrow_up_float"
        android:contentDescription="@string/ib_system_menue_contentDescription" />

    <ImageButton
        android:id="@+id/ib_online_activity"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_gravity="top|right"
        android:src="@android:drawable/stat_sys_download"
        android:contentDescription="@string/ib_online_activity_contentDescription" />

</FrameLayout>
